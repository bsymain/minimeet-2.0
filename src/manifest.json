{
    "name": "MiniMeet",
    "version": "1.0.9",
    "description": "Automatically join meetings on your calendar.",
    "permissions": [
        "https://meet.google.com/",
        "https://accounts.google.com/",
        "http://localhost:8090/"
    ],
    "background": {
        "scripts": [
            "extension/background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Join the next meeting on your calendar."
    },
    "manifest_version": 2
}
